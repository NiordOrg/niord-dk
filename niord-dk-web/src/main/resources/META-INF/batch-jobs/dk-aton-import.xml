<job id="DkAtonImportJob" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
    <step id="setupStep" allow-start-if-complete="true" next="importAtonsStep">
        <batchlet ref="jobStartBatchlet"/>
    </step>
    <step id="importAtonsStep">
        <chunk item-count="10">
            <reader ref="batchAtonReader" />
            <processor ref="batchAtonProcessor"/>
            <writer ref="batchAtonWriter" />
        </chunk>
    </step>
</job>
